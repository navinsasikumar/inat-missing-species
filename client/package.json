{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "^4.6.0",
    "node-sass": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "query-string": "^6.14.1",
    "react": "^16.14.0",
    "react-bootstrap": "^1.5.2",
    "react-dom": "^16.14.0",
    "react-infinite-scroller": "^1.2.4",
    "react-loader-spinner": "^3.1.14",
    "react-paginate": "^6.5.0",
    "react-router-bootstrap": "^0.25.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^4.0.3",
    "react-tooltip": "^4.2.15",
    "styled-components": "^4.4.1"
  },
  "scripts": {
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass --include-path ./src  --include-path ./node_modules src/ -o src/compiled",
    "watch-css": "npm run build-css && node-sass --include-path ./src --include-path ./node_modules src/ -o src/compiled/ --watch",
    "start-js": "set HTTPS=true&&react-scripts start",
    "build-js": "react-scripts build"
  },
  "proxy": "http://server:8080",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "bufferutil": "^4.0.3",
    "fibers": "^5.0.0",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1",
    "sass": "^1.32.8",
    "ts-node": "^9.1.1",
    "type-fest": "^0.13.1",
    "typescript": "^3.9.9",
    "utf-8-validate": "^5.0.4",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-plugin-serve": "^0.12.1"
  }
}
